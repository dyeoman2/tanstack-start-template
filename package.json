{
  "name": "tanstack-start-starter",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "license": "MIT",
  "author": "Daniel Yeoman",
  "version": "1.0.0",
  "description": "TanStack Start template built with Better Auth, Convex, Tailwind CSS, Shadcn/UI, Resend, and deployed to Netlify",
  "homepage": "https://github.com/dyeoman2/tanstack-start-template",
  "repository": {
    "type": "git",
    "url": "https://github.com/dyeoman2/tanstack-start-template.git"
  },
  "engines": {
    "node": ">=24"
  },
  "keywords": [
    "tanstack start",
    "react",
    "typescript",
    "starter",
    "template",
    "fullstack",
    "authentication",
    "convex",
    "resend",
    "netlify"
  ],
  "scripts": {
    "dev": "vite dev",
    "dev:full": "tsx scripts/dev-full.ts",
    "build": "vite build",
    "start": "node ./.output/server/index.mjs",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check .",
    "fix": "biome check --write .",
    "knip": "knip",
    "knip:prod": "knip --production",
    "setup": "tsx scripts/setup.ts",
    "precommit": "biome check --write",
    "updates": "npx npm-check-updates",
    "analyze": "pnpm build && npx vite-bundle-analyzer dist/client/assets/*.js",
    "infra:deploy": "tsx infra/storage.ts",
    "infra:preview": "tsx infra/storage.ts --preview"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.920.0",
    "@convex-dev/better-auth": "^0.9.6",
    "@convex-dev/resend": "^0.1.13",
    "@netlify/vite-plugin-tanstack-start": "^1.1.8",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-form": "^1.23.8",
    "@tanstack/react-router": "^1.133.36",
    "@tanstack/react-router-devtools": "^1.133.36",
    "@tanstack/react-start": "^1.134.0",
    "@tanstack/react-table": "^8.21.3",
    "better-auth": "^1.3.34",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.28.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.548.0",
    "next-themes": "^0.4.6",
    "npm-check-updates": "^19.1.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.6",
    "vite": "7.1.12",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@aws-sdk/client-iam": "^3.920.0",
    "@biomejs/biome": "^2.3.2",
    "@tailwindcss/vite": "^4.1.16",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "knip": "^5.66.4",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
